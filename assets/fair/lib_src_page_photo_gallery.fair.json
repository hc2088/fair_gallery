{
  "className": "Scaffold",
  "na": {
    "appBar": {
      "className": "AppBar",
      "na": {
        "title": {
          "className": "Text",
          "pa": [
            "PhotoGallery"
          ]
        }
      }
    },
    "body": {
      "className": "PullToRefreshNotification",
      "na": {
        "pullBackOnRefresh": false,
        "maxDragOffset": 90,
        "armedDragUpCancel": false,
        "onRefresh": "@(_onRefresh)",
        "child": {
          "className": "Column",
          "na": {
            "children": [
              {
                "className": "ShareDataWidget",
                "na": {
                  "data": "^(_repository)",
                  "child": {
                    "className": "PullToRefreshContainer",
                    "pa": [
                      {
                        "className": "SugarCommon.pullToRefreshContainerBuilder"
                      }
                    ]
                  }
                }
              },
              {
                "className": "Expanded",
                "na": {
                  "child": {
                    "className": "SizeCacheWidget",
                    "na": {
                      "child": {
                        "className": "ExtendedTextSelectionPointerHandler",
                        "na": {
                          "child": {
                            "className": "LoadingMoreList",
                            "pa": [
                              {
                                "className": "ListConfig",
                                "na": {
                                  "itemBuilder": "@(_itemBuilder)",
                                  "sourceList": {
                                    "className": "LoadingMoreRepository.onLoadData",
                                    "pa": [
                                      "^(_repository)",
                                      "@(_onLoadData)"
                                    ],
                                    "na": {
                                      "maxLength": 300
                                    }
                                  },
                                  "cacheExtent": {
                                    "className": "SugarNum.toDouble",
                                    "pa": [
                                      {
                                        "className": "SugarNum.multiplies",
                                        "pa": [
                                          {
                                            "className": "Sugar.height",
                                            "pa": [
                                              "^(context)"
                                            ]
                                          },
                                          2
                                        ]
                                      }
                                    ]
                                  },
                                  "physics": {
                                    "className": "BouncingScrollPhysics"
                                  },
                                  "indicatorBuilder": {
                                    "className": "SugarCommon.loadingMoreIndicatorBuilder",
                                    "pa": [
                                      [
                                        {
                                          "className": "SugarSwitchCaseObj",
                                          "na": {
                                            "sugarCase": "#(IndicatorStatus.fullScreenBusying)",
                                            "reValue": {
                                              "className": "Align",
                                              "na": {
                                                "alignment": "#(Alignment.center)",
                                                "child": {
                                                  "className": "Text",
                                                  "pa": [
                                                    "不要着急，正在加载中..."
                                                  ]
                                                }
                                              }
                                            }
                                          }
                                        },
                                        {
                                          "className": "SugarSwitchCaseObj",
                                          "na": {
                                            "sugarCase": "#(IndicatorStatus.loadingMoreBusying)",
                                            "reValue": {
                                              "className": "Container",
                                              "na": {
                                                "height": 35.0,
                                                "alignment": "#(Alignment.center)",
                                                "child": {
                                                  "className": "Text",
                                                  "pa": [
                                                    "不要拖了，正在加载更多..."
                                                  ]
                                                }
                                              }
                                            }
                                          }
                                        },
                                        {
                                          "className": "SugarSwitchCaseObj",
                                          "na": {
                                            "sugarCase": "#(IndicatorStatus.noMoreLoad)",
                                            "reValue": {
                                              "className": "Container",
                                              "na": {
                                                "height": 35.0,
                                                "alignment": "#(Alignment.center)",
                                                "child": {
                                                  "className": "Text",
                                                  "pa": [
                                                    "拖不动了，没有更多了！"
                                                  ]
                                                }
                                              }
                                            }
                                          }
                                        }
                                      ]
                                    ],
                                    "na": {
                                      "isSliver": false
                                    }
                                  }
                                }
                              }
                            ]
                          }
                        }
                      }
                    }
                  }
                }
              }
            ]
          }
        }
      }
    }
  },
  "methodMap": {
    "_itemBuilder": {
      "className": "FrameSeparateWidget",
      "na": {
        "index": "^(index)",
        "child": {
          "className": "ExtendedFairWidget",
          "na": {
            "builder": {
              "className": "PhotoGalleryItem",
              "na": {
                "fairProps": "^(fairProps)"
              }
            },
            "name": "$(name:)",
            "fairProps": "^(fairProps)",
            "holder": {
              "className": "SizedBox",
              "na": {
                "height": "^(height)"
              }
            }
          }
        },
        "placeHolder": {
          "className": "SizedBox",
          "na": {
            "height": "^(height)"
          }
        }
      }
    },
    "_onRefresh": {
      "className": "_repository.refresh"
    }
  }
}